{
	"build": {
		"beforeBuildCommand": "npm run build",
		"beforeDevCommand": "npm run dev",
		"devPath": "http://localhost:5173",
		"distDir": "../build"
	},
	"package": {
		"productName": "Time Tracker",
		"version": "1.0.0"
	},
	"tauri": {
		"allowlist": {
			"all": false,
			"fs": {
				"all": false,
				"createDir": true,
				"exists": true,
				"readFile": true,
				"writeFile": true,
				"scope": ["$APPDIR/**"]
			},
			"path": {
				"all": true
			}
		},
		"bundle": {
			"active": true,
			"category": "Productivity",
			"copyright": "",
			"deb": {
				"depends": []
			},
			"externalBin": [],
			"icon": [
				"icons/icon_32x32.png",
				"icons/icon_128x128.png",
				"icons/icon_128x128@2x.png",
				"icons/icon.icns",
				"icons/icon.ico"
			],
			"identifier": "com.aabishkaryal.time-tracker",
			"longDescription": "",
			"macOS": {
				"entitlements": null,
				"exceptionDomain": "",
				"frameworks": [],
				"providerShortName": null,
				"signingIdentity": null
			},
			"resources": [],
			"shortDescription": "",
			"targets": "all",
			"windows": {
				"certificateThumbprint": null,
				"digestAlgorithm": "sha256",
				"timestampUrl": ""
			}
		},
		"security": {
			"csp": null
		},
		"updater": {
			"active": false
		},
		"windows": [
			{
				"fullscreen": false,
				"resizable": true,
				"title": "Time Tracker",
				"visible": true,
				"hiddenTitle": false,
				"decorations": true,
				"focus": false,
				"transparent": false
			}
		],
		"systemTray": {
			"iconPath": "icons/icon_512x512.png",
			"iconAsTemplate": true
		},
		"macOSPrivateApi": true
	}
}
